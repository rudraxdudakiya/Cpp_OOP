#include <iostream>
using namespace std;

class Time
{
	int Hour, Minute, Second;
public:
	void get_input()
	{
		printf("Enter Hour : ");
		scanf("%d" ,&Hour);
		printf("Enter Minutes : ");
		scanf("%d" ,&Minute);
		printf("Enter Second : ");
		scanf("%d",&second);
	}
};
int main()
{
	return 0;
}

